<script src="js/graph.js"></script>

<form action="/graph" method="get">
  <input title="Window width"
         name="window_width"
         type="text"
         value="<%= window_width %>"/>
  <input title="Give up after"
         name="give_up_after"
         type="text"
         value="<%= give_up_after %>"/>
  <input type="submit" value="Submit" />
</form>

<div  id="container"
      style="height: 400px; min-width: 600px"
      data-high=<%= high.to_json %>
      data-open=<%= open.to_json %>
      data-close=<%= close.to_json %>
      data-low=<%= low.to_json %>
      data-avg20d=<%= avg20d.to_json %>
      data-avg50d=<%= avg50d.to_json %>
      data-collections=<%= collections.to_json %>
    >
  Loading...
</div>
